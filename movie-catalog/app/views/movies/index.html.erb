<h1>Movie Catalog</h1>
<h2>All movies</h2>
<%= link_to 'Register a new movie!', new_movie_path %>
<ul>
    <% @movies.each do |m| %>
        <li>
            <h3><%= m.title %></h3>
            <p><%= m.release_date.year %></p>
            <%= link_to 'See more', movie_path(m) %>
        </li>
    <% end %>
</ul>